URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard
 
URxvt.keysym.M-x           : perl:keyboard-select:activate
URxvt.keysym.M-u           : perl:url-select:select_next
URxvt.keysym.M-c           : perl:clipboard:copy
URxvt.keysym.M-v           : perl:clipboard:paste
URxvt.keysym.M-s           : perl:keyboard-select:search
URxvt.keysym.M-C-v         : perl:clipboard:paste_escaped
URxvt.url-select.launcher  : firefox
URxvt.url-select.autocopy  : true
URxvt.url-select.underline : true
 
URxvt.geometry  : 145x43
 
URxvt.font        : xft:DejaVu Sans Mono for Powerline:pixelsize=20
URxvt.letterSpace : 0
URxvt.termName    : rxvt-256color
URxvt.iso14755    : true
URxvt*depth: 32
URxvt.foreground: 15
 
URxvt.urgentOnBell: true
 
URxvt.saveLines   : 12000
URxvt.scrollBar   : false
URxvt.scrollstyle : rxvt
 
!XFT
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb
 
!Solarized + tangerine + random colours
URxvt.cursorColor            : #93A1A1
URxvt*cursorBlink:      true
 
URxvt.pointerColorBackground : #586E75
URxvt.urlLuncher:     /usr/bin/iceweasel
URxvt.pointerColorForeground : #93A1A1
 
URxvt.Depth : 32
##URxvt.background : #404040
URxvt*background : rgba:0000/0000/0200/c800
URxvt.foreground : #F6F3E8
#URxvt*.transparent  : True
#URxvt*.shading: 20
 
URxvt.colorBD : #FFFFFF
URxvt.colorUL : #CCCC66
 
